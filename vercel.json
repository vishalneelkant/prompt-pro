{
  "version": 2,
  "builds": [
    {
      "src": "src/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
